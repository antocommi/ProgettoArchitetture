gcc -O0 -m32 gemm0.c -o gemm0

gcc -O0 -m32 gemm1.c -o gemm1

nasm -f elf32 gemm1asm.nasm
gcc -O0 -m32 gemm1asm.o -o gemm1asm

nasm -f elf32 gemm2.nasm
gcc -O0 -m32 gemm2.o -o gemm2

nasm -f elf32 gemm2jik.nasm
gcc -O0 -m32 gemm2jik.o -o gemm2jik

nasm -f elf32 gemm3.nasm
gcc -O0 -m32 gemm3.o -o gemm3

nasm -f elf32 gemm4.nasm
gcc -O0 -m32 gemm4.o -o gemm4

nasm -f elf32 gemm5.nasm
gcc -O0 -m32 -msse -fopenmp gemm5.o gemm5c.c -o gemm5c

echo '********** Matrix Multiplication test **********'

echo 'Assembly code + Code Vectorization SSE + Loop Unrolling + Cache-blocking + Parallel-for'
time ./gemm5c

echo 'Assembly code + Code Vectorization SSE + Loop Unrolling + Cache-blocking'
time ./gemm4

echo 'Assembly code + Località degli accessi + Code Vectorization SSE + Loop Unrolling'
time ./gemm3

echo 'Assembly code + Località degli accessi + Code Vectorization SSE'
time ./gemm2

echo 'Assembly code + Code Vectorization SSE (no località degli accessi)'
time ./gemm2jik

echo 'Assemly code + Località degli accessi + istruzioni scalari SSE'
time ./gemm1asm

echo 'C code + Località degli accessi'
time ./gemm1

echo 'C code + array di array'
time ./gemm0
